isteration from './pages/Registeration';\nimport Profile from './pages/Profile';\nimport Logout from './pages/Logout';\n\nfunction App() {\n  const dispatch = useDispatch()\n  const token = useSelector((state) => state.userReducer.token)\n  const searchParam = useSelector((state) => state.searchParamReducer.searchParam)\n  const [userData,setUserData] = useState()\n  useEffect(()=>{\n    axiosClient.get(`/user`, {\n      headers: {\n        'Content-Type': 'application/json',\n        'Authorization': `Bearer ${token}`\n      }\n    }).then(resp => {\n      if(resp.status == 200){\n        dispatch(setUser(resp.data))\n        setUserData(resp.data)\n        axiosClient.get(`/user-preferences`, {\n          headers: {\n            'Content-Type': 'application/json',\n            'Authorization': `Bearer ${token}`\n          },\n        }).then(resp => {\n          if(resp.status == 200){\n            dispatch(setSearchParam({\n              search: searchParam.search,\n              date: searchParam.date,\n              category: resp.data.preferences.category,\n              source: resp.data.preferences.source\n            }))\n          }\n        })\n      }else{\n        dispatch(setUser(null))\n        setUserData(undefined)\n      }\n    })\n  },[token])\n  return (\n    <>\n      <Header/>\n      <Router>\n        <Routes>\n          <Route path=\"\" element={<Home />} />\n          { userData ?\n            <>\n              <Route path=\"/profile\" element={<Profile />} />\n              <Route path=\"/logout\" element={<Logout />} />\n            </>\n            :\n            <>\n              <Route path=\"/registration\" element={<Registeration />} />\n              <Route path=\"/login\" element={<Login />} />\n            </>\n          }\n        </Routes>\n      </Router>\n      <Footer/>\n    </>\n  );\n}\n\nexport default App;\n","/app/frontend/src/reportWebVitals.js",[],[],"/app/frontend/src/redux/store.js",[],[],"/app/frontend/src/redux/searc